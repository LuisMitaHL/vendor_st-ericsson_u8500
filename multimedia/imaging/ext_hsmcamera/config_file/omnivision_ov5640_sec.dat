PLL_CONTROL_OV5640 =
  $">> CAMDRIVER::PLL control seq for OV5640 camera module"
    3103 11 $f    //PLL
    3008 82 $f
    $d000a
    3008 42 $f
    $d000a
    3103 03 $f
    3017 00 $f
    3018 00 $f
    3034 18 $f
    3035 12 $f
    3036 3f $f
    3037 13 $f
    303b 19 $f
//    303c 11 $f default value is already 11
    303d 30 $f
    3108 01 $f
  $"<< CAMDRIVER::PLL control seq for OV5640 camera module"

; // PLL_CONTROL_OV5640_END

