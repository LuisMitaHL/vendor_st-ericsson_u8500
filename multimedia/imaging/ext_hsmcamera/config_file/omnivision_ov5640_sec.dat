RESET_PW_DOWN_OV5640 =
  $">> CAMDRIVER::Power Down sequence for OV5640 camera module"
      
      300e 28 $f        // MIPI CONTR0L0 - MIPI Disable and Power Down
      3103 11 $f        // SYSTEM CTRL1 - debug mode
      3008 82 $f        // SYSTEM CTROL0 -  software power down
      3008 42 $f        // SYSTEM CTROL0 -  software reset
      3000 30 $f        // System Reset00
      3001 08 $f        // System Reset01
      3002 1c $f        // System Reset02
      3003 00 $f        // System Reset03
      3004 cf $f        // Clock Enable00
      3005 f7 $f        // Clock Enable01
      3006 e3 $f        // Clock Enable02
      3007 ff $f        // Clock Enable03

  $"<< CAMDRIVER::Power Down sequence for OV5640 camera module"
; //RESET_PW_DOWN_OV5640

SET_MIPI_CTRL =     
    4800 04 $f
    4202 00 $f
    $"<< CAMDRIVER::SET OV5640 MIPI CTRL"
; //SET_MIPI_CTRL

RESET_MIPI_CTRL =     
    4800 24 $f
    4202 0f $f
    $"<< CAMDRIVER::RESET OV5640 MIPI CTRL"
; //RESET_MIPI_CTRL

ENTER_MIPI_STANDBY = 
    $">> CAMDRIVER::ENTER OV5640 MIPI STANDBY"

    300e 28 $f        // MIPI CONTR0L0 - MIPI hw and software standby (1DL)

    $"<< CAMDRIVER::ENTER OV5640 MIPI STANDBY"
; //ENTER_MIPI_STANDBY_END

EXIT_MIPI_STANDBY = 
    $">> CAMDRIVER::EXIT OV5640 MIPI STANDBY"

    300e 25 $f        // MIPI CONTR0L0 - OUT OF MIPI HW and software standby (1DL)
    
    $"<< CAMDRIVER::EXIT OV5640 MIPI STANDBY"
; //EXIT_MIPI_STANDBY_END

STOP_SENSOR_OV5640 =
    $">> CAMDRIVER::STOP OV5640 camera module"

    3008 42 $f        // SYSTEM CTROL0 -  software power down

    $"<< CAMDRIVER::STOP OV5640 camera module"
; //STOP_SENSOR_OV5640_END

START_SENSOR_OV5640 =
    $">> CAMDRIVER::START OV5640 camera module"

    3008 02    $f    //System control , OUT OF RESET and STREAMING

