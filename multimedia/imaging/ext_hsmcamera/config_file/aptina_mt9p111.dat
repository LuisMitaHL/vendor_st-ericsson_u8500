REFRESH_P=

  $">> CAMDRIVER::REFRESH_P"

  098E 8404 $f          // Refresh Sequencer Mode
  8404 05   $f          // = 5
  $d0014
  
  #POLL_SEQ_CMD_DONE
  
  $"<< CAMDRIVER::REFRESH_P"

; // REFRESH_P

