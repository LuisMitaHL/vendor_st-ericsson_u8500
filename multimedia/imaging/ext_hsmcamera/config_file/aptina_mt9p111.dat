ENTER_CAPTURE_MODE=

  $">> CAMDRIVER::ENTER_CAPTURE_MODE"

  098E 843C $f 	// LOGICAL_ADDRESS_ACCESS [SEQ_STATE_CFG_5_MAX_FRAME_CNT]
  843C FF   $f	// SEQ_STATE_CFG_5_MAX_FRAME_CNT
  8404 02   $f	// SEQ_CMD
  $d0032
  #CHECK_CAPTURE_MODE
  
  $"<< CAMDRIVER::ENTER_CAPTURE_MODE"

; // ENTER_CAPTURE_MODE

