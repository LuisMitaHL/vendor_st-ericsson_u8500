  $s4 01
  [4 64             // loop
    098E 8405 $f
    #GET_REG(0990)
    $c1 00       // compare with 0 (return 0 for init)
      $s4 00     // done
    $cl
      $d000a     // else not done, delay
    $cx
  ]
  $c4 00
  $cl
