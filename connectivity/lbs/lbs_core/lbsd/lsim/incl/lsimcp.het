/*----------------------------------------------------------------------*/
/*!
    \struct t_lsimcp_RefTime
    \brief GPS Reference Time
    \note Macros used : \ref Type_of_UNKNOWN
*/

typedef struct {
    t_lsimcp_TowAssist      a_TowAssist[LSIMCP_MAX_SAT_NR];     /**<GPS TOW Assist consists of TLM message, Anti-spoof flag, Alert flag, and 2 reserved bits in TLM Word. \ref LSIMCP_MAX_SAT_NR*/
    uint32_t                     v_GpsTow;                       /**<Time of Week (milliseconds)
                                                                Note: In GSM the received value need to be multiplies by 80ms to convert to this range.
                                                                In UTRAN no change is required.*/
    uint32_t                     v_Prec;                         /**<Microseconds
                                                                Precision of the time calibration required in RMS.*/
    int32_t                     v_RefDeltaCellTime;             /**<This parameter shall store the Delta time with respect to GPS time when the Assistance had been obtained.
                                                                Delta is the difference of the UTRAN/GSM time sent by Network to when synchronization pulse is sent to the GPS baseband chip.
                                                                0x7FFFFFFF  Unknown \ref Type_of_UNKNOWN
                                                                If no UTRAN/GSM Time available, the value shall be set to 0*/
    uint16_t                     v_GpsWeek;                      /**<GPS Week Number*/
    uint8_t                      v_SfnTowUnc ;                   /**<SFN-TOW Uncertainty.
                                                                255  Unknown \ref Type_of_UNKNOWN*/
    int8_t                      v_UtranGpsDriftRate;            /**<TUTRAN-GPS drift rate.
                                                                128  Unknown \ref Type_of_UNKNOWN*/
    uint8_t                      v_NumTowAssist;                 /**<Number of TOW assist for each visible satellite*/
    MC_PCC_FILL_STRUCT3                                     /*!< Padding to align on 32 */
}t_lsimcp_RefTime;

