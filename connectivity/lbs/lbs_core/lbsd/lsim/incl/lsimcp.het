}t_lsimcp_TowAssist;

/*----------------------------------------------------------------------*/
/*!
    \struct t_lsimcp_SatEphemData
    \brief Satellite Ephemeris Data
    \note Macros used :
                        - \ref Type_of_Navigation_Model
*/

typedef struct {
    uint8_t                  v_SatId;        /**<Satellite Id. 0-31 indicating GPS satellites 1-32, 32-63 indicating augmentation systems*/
    uint8_t                  v_ExtBit;       /**<CN Interface backward compatibility*/
    uint16_t                 v_SatStatus;    /**<Satellite Status.
                                            \ref Type_of_Navigation_Model */
    uint8_t                  v_EphemCodeOnL2;/**<CA or P on L2*/
    uint8_t                  v_EphemURA;     /**<URA(User Range Accuracy) Index*/
    uint8_t                  v_EphemSvHealth;/**<SV Health*/
    uint8_t                  v_Epheml2pFlag; /**<L2P data flag*/
    uint16_t                 v_EphemIodc;    /**<IODC (Issue of Data Clock)*/
    uint16_t                 v_EphemToc;     /**<Apparent clock correction*/
    uint32_t                 v_Reserved1;    /**<SF(Sub-Frame) 1 Reserved*/
    uint32_t                 v_Reserved2;    /**<SF 1 Reserved*/
    uint32_t                 v_Reserved3;    /**<SF 1 Reserved*/
    uint32_t                 v_Reserved4;    /**<SF 1 Reserved*/
    int8_t                  v_EphemTgd;     /**<Estimated Group Delay differential*/
    int8_t                  v_EphemAf2;     /**<Apparent clock correction */
    int16_t                 v_EphemAf1;     /**<apparent clock correction*/
    int32_t                 v_EphemAf0;     /**<apparent clock correction*/
    int16_t                 v_EphemCrs;     /**<Amplitude of the Sine Harmonic Correction Term to the Orbit Radius */
    int16_t                 v_EphemDeltaN;  /**<Mean Motion Difference From Computed Value */
    int32_t                 v_EphemMo;      /**<Mean Anomaly at Reference Time */
    int16_t                 v_EphemCuc;     /**<Amplitude of the Cosine Harmonic Correction Term To The Argument Of Latitude */
    int16_t                 v_EphemCus;     /**<Amplitude of the Sine Harmonic Correction Term To The Argument Of Latitude */
    uint32_t                 v_Epheme;       /**<Eccentricity*/
    uint32_t                 v_EphemAPowerhalf;  /**<Semi-Major Axis */
    uint16_t                 v_EphemToe;     /**<Reference Time Ephemeris*/
    uint8_t                  v_EphemFitFlag; /**< */
    uint8_t                  v_EphemAODO;    /**<Age Of Data Offset*/
    int16_t                 v_EphemCic;     /**<Amplitude of the Cosine Harmonic Correction Term To The Angle Of Inclination */
    int16_t                 v_EphemCis;     /**<Amplitude of the Sine Harmonic Correction Term To The Angle Of Inclination */
    int32_t                 v_EphemOmegaA0; /**<Longitude of Ascending Node of Orbit Plane at Weekly Epoch*/
    int32_t                 v_EphemIo;      /**<Inclination Angle at Reference Time*/
    int16_t                 v_EphemIdot;    /**<Rate of Inclination Angle */
    int16_t                 v_EphemCrc;     /**<Amplitude of the Cosine Harmonic Correction Term to the Orbit Radius */
    int32_t                 v_EphemW;       /**<Argument of Perigee */
    int32_t                 v_EphemOmegaAdot;   /**<Longitude of Ascending Node of Orbit Plane at Weekly Epoch */
}t_lsimcp_SatEphemData;


