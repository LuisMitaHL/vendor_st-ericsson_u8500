/*----------------------------------------------------------------------*/
/*!
    \struct t_lsimcp_GpsPosition
    \brief Position estimate
    \note  Macros Used :
                        - \ref Type_of_FIX
                        - \ref Type_of_Shape
                        - \ref Type_of_Latitude_Sign
                        - \ref Type_of_Altitude_Direction
                        - \ref Type_of_UNKNOWN
*/

typedef struct {
    uint8_t                  v_Status;           /**<Position Validity Status,Returns CGPS_LSIMCP_OK, CGPS_LSIMCP_ERR*/
    uint8_t                  v_FixType;          /**<cPosMode geographical information 0-2D, 1-3D \ref Type_of_FIX*/
    uint16_t                 v_RefFrame;         /**<GSM Reference Time
                                                65535 - Unknown \ref Type_of_UNKNOWN*/


    uint16_t                v_GpsTowSubms;      /*GPS Reference Time in microseconds
                                                                                              Valid range:0-9999
                                                                                              Value interpretation:TOW in ms=GPS TOW + .0001*GPSTOWSubms
                                                                                              Value not present:0xFFFF */
    MC_PCC_FILL_STRUCT2                         /*!< Padding to align on 32 */
    int32_t                 v_RefDeltaCellTime; /**<Microsecond
                                                This shall store the reference cell time with respect to the time of synchronization pulse input to the GPS baseband chip
                                                0x7FFFFFFF- Unknown \ref Type_of_UNKNOWN*/
    uint32_t                 v_GpsTow;           /**<Measurement GPS Time of Week (milliseconds).  INTEGER (0..604799999).
                                                            **/
    

    uint32_t                 v_GpsTowUnc;        /**<GPS Reference Time Uncertainty in microseconds
                                                                                        0xFFFFFFFF - Unknown \ref Type_of_UNKNOWN
                                                0xFFFFFFFF - Unknown \ref Type_of_UNKNOWN*/                                                                                     
    uint32_t                 v_Latitude;         /*!< Latitude (90/2^23 degrees).  INTEGER (0..8388607).  Represents 0..90 degrees.
                                                            Value as per the GAD format used in the RRC and RRLP specifications. */
    int32_t                 v_Longitude;       /*!< Longitude (180/2^23 degrees).  INTEGER (-8388608..8388607).   Represents -180..+180 degrees.
                                                            Value as per the GAD format used in the RRC and RRLP specifications. */
    uint8_t                  v_ShapeType;        /**<\ref Type_of_Shape
                                                0 - Ellipsoid Point
                                                1 - EP with uncertainty circle
                                                3 - EP with uncertainty Ellipse
                                                8 - EP with altitude
                                                9 - EP with altitude and uncertainty Ellipsoid
                                                10 - EP arc*/
    uint8_t                  v_SignOfLat;        /**<0 - North ,
                                                1 - South \ref Type_of_Latitude_Sign Value as per the GAD format used in the RRC and RRLP specifications. */
    uint8_t                  v_UncertSemiMajor;  /**<Valid Shapes: 1,3,9,10 Shape 1: Major Axis shall store Uncertainity Code   INTEGER (0..127). Shape 10: Major Axis shall be interpreted as Uncertainity Radius 255 - Unknown \ref Type_of_UNKNOWN Value as per the GAD format used in the RRC and RRLP specifications. */
    uint8_t                  v_UncertSemiMinor;  /**<Valid Shapes: 3,9.   INTEGER (0..127).  255 - Unknown \ref Type_of_UNKNOWN Value as per the GAD format used in the RRC and RRLP specifications*/
    uint32_t                 v_Altitude;         /**<Valid Shapes: 8,9. < Altitude (metres).  INTEGER (0..32767).
                                                            <br>Value as per the GAD format used in the RRC and RRLP specifications. */
    uint32_t                 v_InnerRadius;      /**<Valid Shapes: 10. Meters 0xFFFFFFFF - Unknown \ref Type_of_UNKNOWN */
    uint8_t                  v_AxisAngle;        /**<Valid Shapes: 3,9.255 - Unknown \ref Type_of_UNKNOWN*/
    uint8_t                  v_DirectOfAlt;      /**<Valid Shapes: 8,9.
                                                0 - height,
                                                1 - depth \ref Type_of_Altitude_Direction
                                                -1=Altitude not present*/

    uint8_t                  v_UncertAltitude;   /**<Valid Shapes: 9. INTEGER (0..127)  255 - Unknown \ref Type_of_UNKNOWN Refer 3GPP 23.032 for Scaling factor,Value as per the GAD format used in the RRC and RRLP specifications.*/
    uint8_t                  v_Confidence;       /**<Valid Shapes: 1,3,9,10.  Percent    255  Unknown \ref Type_of_UNKNOWN,Value as per the GAD format used in the RRC and RRLP specifications*/
    uint8_t                  v_IncludedAngle;    /**<Valid Shapes: 10. 255 - Unknown \ref Type_of_UNKNOWN  2 N <= ai < 2 (N+1)       Accepted values for ai(angle included) are within the range from 0 to 359,9...9 degrees.Where N is coded number provided in Offset Angle ,Value as per the GAD format used in the RRC and RRLP specifications*/
    uint8_t                  v_OffsetAngle;      /**<Valid Shapes: 10. 255 - Unknown \ref Type_of_UNKNOWN  2 N <= ao < 2 (N+1)       Accepted values for ao(angle offset) are within the range from 0 to 359,9...9 degrees.Where N is coded number provided in Offset Angle Value as per the GAD format used in the RRC and RRLP specifications*/

    uint8_t                  v_PolygonNumOfPoints;  /**<Valid Shapes: 5. This can vary from 3 to 15 if polygon is the valid shape type. If value is 0, invalid polygon */
    uint8_t                  a_PolygonSignOfLat[LSIMCP_MAX_POLYGON_POINTS];  /**<Valid Shapes: 5. 0 - North , 1 - South \ref Type_of_Latitude_Sign*/
         
    uint32_t                 a_PolygonLatitude[LSIMCP_MAX_POLYGON_POINTS];   /**<Valid Shapes: 5. Latitude in radian*/
    int32_t                 a_PolygonLongitude[LSIMCP_MAX_POLYGON_POINTS];  /**<Valid Shapes: 5. Longitude in radian*/


}t_lsimcp_GpsPosition;

