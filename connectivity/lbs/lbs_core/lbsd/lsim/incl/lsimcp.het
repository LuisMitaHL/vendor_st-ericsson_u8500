/*!
    \struct t_lsimcp_GpsAidRequest
    \brief Aid identification
    \note  Macros Used : \ref Bitmask_for_t_lsimcp_AidRequest
                         \ref Type_of_UNKNOWN
*/

typedef struct {
    uint32_t                 v_Latitude;     /**<Latitide in radian*/
    int32_t                 v_Longitude;    /**<Latitude in radian*/
    uint32_t                 v_Altitude;     /**<Altitude in meters*/
    uint16_t                 v_AidMask;      /**<Bitmask for requested aiding data \ref Bitmask_for_t_lsimcp_AidRequest
                                        Bit 0-Almanac
                                        Bit 1-UTC Model
                                        Bit 2-Ionospheric model
                                        Bit 3-Navigation Model
                                        Bit 4-DGPS Corrections
                                        Bit 5-Reference Location
                                        Bit 6-Reference Time
                                        Bit 7-Acquisition Assistance
                                        Bit 8-Real-Time Integrity
                                        Bit 9-15 reserved
                                        Note: Below fields are valid if Bit 3 are set.*/
    uint16_t                 v_GpsWeek;      /**<GPS Week*/
    uint8_t                  v_GpsTow;       /**<GPS time of ephemeris in hours of the latest ephemeris set contained by the UE.*/
    uint8_t                  v_NrOfSats;     /**<Number of Satellite Information*/
    uint8_t                  v_TTOELimit;    /**<ephemeris age tolerance of the UE to UTRAN in hours
                                        255 - Unknown \ref Type_of_UNKNOWN*/
    uint8_t                  a_SatID[LSIMCP_MAX_SAT_NR];/**<Satellite Id. \ref LSIMCP_MAX_SAT_NR*/
    uint8_t                  a_Iode[LSIMCP_MAX_SAT_NR];  /**<Issue of Data Ephemeris for SatID. \ref LSIMCP_MAX_SAT_NR */
    uint8_t                  v_LocationMethod;   /**<Indicates the Location Method. \ref Type_of_Location_Method */
}t_lsimcp_GpsAidRequest;


