/*    MC_PCC_FILL_STRUCT2                                */
    MC_PCC_FILL_STRUCT1                                        /*!< Padding to align on 32 */
/* -LMSqc34406*/
MC_PCC_END_STRUC   (t_lsimcp_MSAssistedReqM)

