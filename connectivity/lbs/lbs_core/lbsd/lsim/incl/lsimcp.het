/*!
    \struct t_lsimcp_AcquisData
    \brief ---
    \note Macros used : \ref Type_of_UNKNOWN
*/

typedef struct {
    uint8_t                  v_SatId;        /**<Satellite Id. 0-31 indicating GPS satellites 1-32, 32-63 indicating augmentation systems */
