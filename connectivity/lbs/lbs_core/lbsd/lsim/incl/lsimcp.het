/*!
 *  \struct s_gnsCP_FtaRsp
 *  \brief  A-GPS Control Plane Fine TimeData.
 */
typedef struct {

     uint32_t   v_FrameNumber;      /*!<  frame_number    Value range: 0 to 2715647 */
     uint16_t   v_QBitNumber;        /*!<   Bit number (in Quarterbits) Valid Range [0 to 625]. Represent the quadra bit value.*/
     uint16_t   v_BCCHCarrier;      /*!<   bcch_carrier     Valid Rane: [0 to 1023] */
     uint8_t    v_TimeSlot;         /*!<   time_slot   Valid Range [0 to 7] */
     uint8_t    v_TimingAdvance;    /*!<   Valid Range [0 to 63] */
     uint8_t    v_BSIC;             /*!<   bsic  Value range: 0 to 63 */
 } t_lsimCP_GsmCellTime;


/*----------------------------------------------------------------------*/
/*!
 *  \struct s_gnsCP_FtaRsp
 *  \brief  A-GPS Control Plane Fine TimeData.
 */
typedef struct {


    uint16_t   v_CellFrameTimeMS;
    uint32_t   v_CellFrameTimeLS; 
    uint8_t    v_OperationStatus;   /*status    Constant: MODEM_LCS_W_CS_STATUS (from 001.000)
                                                             Unspecified values for "Operation status"should be treated as unknown failure reasons */
    uint16_t   v_Sfn;               /*sfn    0x0000-0x0FFF: Real_val = Field_val
                                                 0x1000-0xFFFE: Illegal SFN
                                                  0xFFFF-0xFFFF: MODEM_LCS_NOT_AVAILABLE */
